
#include<iostream>
#include<iomanip>
#include<cmath>

using namespace std;


int main()
{
	int min, max;
	int ab[10];
	cout << "Podaj 10 liczb: " << endl;
	for (int i = 0; i < 10 ; i++)
	{
		cin >> ab[i];
	}
	min = ab[0];
	max = ab[0];
	for (int i = 0; i < 10; i++)
	{
		if (ab[i] < min)
			min = ab[i];
		if (ab[i] > max)
			max = ab[i];
	}
	cout << "Min: " << min << endl;
	cout << "Max" << max << endl;

	return 0;
}
